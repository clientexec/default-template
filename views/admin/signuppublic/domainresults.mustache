<table class="table table-striped available-domains-to-register table-responsive">
    <thead>
        <tr>
            <th><b class="available-domains-label">{{#translate}}Available Domains{{/translate}}</b></th>
            <th class="text-center"><b class="available-years-label">{{#translate}}Years{{/translate}}</b></th>
            <th>&nbsp;</th>
        </tr>
    </thead>
    <tbody>
        {{#available_options}}
        {{#render_additional}}{{/render_additional}}
        <tr class="domain-option-row">
            <td colspan="3">
                <form class='domainForm' data-domain-name="{{domain_name}}" data-product-id="{{product_id}}" data-domain-type="{{domainType}}" method="POST" parsley-validate>
                    <div class="domain-option-name">{{domain_name}}</div>
                    <div class="domain-option-yrs-wrapper text-center">
                        <select class="domain-option-yrs"></select>
                        <input type="hidden" data-index="{{index}}" class="domain-option-yrs" data-domain-name="{{domain_name}}" data-product-id="{{product_id}}">
                    </div>
                    <div class="domain-option-add text-center">
                        <button class="btn btn-default configure-product" data-domain-name="{{domain_name}}" data-product-id="{{product_id}}" type="button">{{#translate}}Add to cart{{/translate}}</button>
                    </div>
                    <div class="customfields-wrapper"></div>
                    <div class="extra_attributes-wrapper"></div>
                    <div class="addons-wrapper"></div>
                </form>
            </td>
        </tr>
        {{/available_options}}
    </tbody>
</table>


